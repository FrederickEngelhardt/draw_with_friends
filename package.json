{
  "name": "draw_with_friends",
  "version": "0.0.11",
  "private": true,
  "dependencies": {
    "aframe": "^0.8.2",
    "aframe-canvas": "0.0.6",
    "bootstrap": "^4.1.3",
    "react": "^16.8.2",
    "react-chat-window": "^1.0.8",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-sketch": "^0.4.4",
    "reactstrap": "^6.3.1",
    "redux": "^4.0.0",
    "resolve": "1.6.0",
    "socket.io-client": "^2.1.1"
  },
  "scripts": {
    "start": "",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom ./tests/*",
    "deploy": "npm run build && cp build/index.html build/200.html && surge --project ./build/ --domain draw-with-friends.surge.sh",
    "flow": "flow"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-loader": "^8.0.0",
    "chai": "^4.1.2",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-cypress": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-hooks": "^1.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "2.0.8",
    "postcss-preset-env": "^6.3.0",
    "prettier": "^1.16.2",
    "react-test-renderer": "^16.4.2",
    "sinon": "^6.1.5",
    "style-loader": "0.19.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
